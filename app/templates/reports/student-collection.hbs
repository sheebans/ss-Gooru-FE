<div class="controller class analytics collection student">
  <div class="row">
    <div class="col-sm-12 navigation">
      <button {{action "goBack"}} class="btn btn-info back">
          <span>
            {{gru-icon name='keyboard_arrow_left'}}
            {{#if lesson}} {{t "common.back-to-data"}} {{else}} {{t 'common.back'}} {{/if}}
          </span>
      </button>
    </div>
    <div class="col-sm-12">
      {{#if assessmentResult}}
        {{reports.gru-assessment-report assessmentResult=assessmentResult
          onSelectAttempt='selectAttempt'
        }}
      {{/if}}
    </div>
  </div>
</div>

